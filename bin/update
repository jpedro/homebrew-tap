#!/usr/bin/env bash
set -xeuo pipefail

git pp
surge packages https://jpedro-bins.surge.sh

rm -f /Users/pedro/Library/Caches/Homebrew/downloads/6b6e88d49edafaf4b51d7a611a7d283df4d7393a8063633c05e10cae0b188f86--bins.zip
rm -f /Users/pedro/Library/Caches/Homebrew/downloads/8dcb3f00004be0892f60d8084a5b156b4e9189e430074847fc5f55f4207d632f--bins.zip

brew uninstall jpedro/tap/bins || echo "Skipping uninstall"
brew update
brew install jpedro/tap/bins

c1
c0
